<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <item>
        <shape android:shape="oval">
            <size
                android:width="12dp"
                android:height="12dp" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp">
        <shape android:shape="oval">
            <solid android:color="#ff579e" />
        </shape>
    </item>
</layer-list>